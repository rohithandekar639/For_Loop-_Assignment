n = 5

# Right-angled triangle pattern
print("Right-angled Triangle:")
for i in range(n):
    for j in range(i + 1):
        print("*", end=" ")
    print()

#  Right-angled triangle pattern
print("\nInverted Right-angled Triangle:")
for i in range(n, 0, -1):
    for j in range(i):
        print("*", end=" ")
    print()
